{
    "destinationRoot": "/public/themes/{{themeName}}/",
    "tasks": {
        "fonticon": {
            "destination": "fonts/",
            "fileType": "scss",
            "fileName": "./../../../../src/assets/sass/common/_fonticon.scss"
        },
        "sass": {
            "destination": "css/"
        },
        "less": {
            "destination": "css/"
        },
        "postcss": {
            "destination": "css/"
        },
        "jslibs": {
            "destination": "scripts/"
        },
        "scripts": {
            "destination": "scripts/",
            "destinationFile": "app.js"
        },
        "views": {
            "destination": "views/"
        },
        "images": {
            "destination": "images"
        },
        "fonts": {
            "destination": "fonts"
        },
        "svgsprite": {
            "destination": "images"
        }
    }
}